@model List<signedup.Models.User>
@{
    ViewBag.Title = "New Employees";
    Layout = "~/Views/Shared/AdminHomepage.cshtml"; // Apply the layout with the navbar
}

<h2>New Employees</h2>
<table class="table">
    <thead>
        <tr>
            <th>UserId</th>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Role</th>
            <th>Email</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Username</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Role</td>
                <td>@user.Email</td>
                @*<td>@user.SignUpDate</td>*@
            </tr>
        }
    </tbody>
</table>
